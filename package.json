{
  "name": "gatsby-starter-skeleton",
  "license": "MIT",
  "private": true,
  "version": "3.0.0",
  "scripts": {
    "build": "NODE_OPTIONS='--openssl-legacy-provider --max-old-space-size=4096 --max-http-header-size=16384' gatsby build",
    "clean": "gatsby clean",
    "dev": "NODE_OPTIONS='--openssl-legacy-provider --max-old-space-size=4096 --max-http-header-size=16384' gatsby develop --host 0.0.0.0",
    "fix": "npm run fix:js && npm run fix:prettier",
    "fix:js": "npm run lint:js -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:prettier",
    "lint:js": "eslint --ignore-path .gitignore . --ext js --ext ts --ext tsx",
    "lint:css": "stylelint --ignore-path .gitignore './src/**/*.ts'",
    "lint:prettier": "prettier --ignore-path .gitignore --check .",
    "serve": "NODE_OPTIONS='--openssl-legacy-provider --max-old-space-size=4096' gatsby serve --host 0.0.0.0",
    "type-check": "tsc",
    "plop": "plop"
  },
  "dependencies": {
    "@fontsource/oswald": "^5.2.8",
    "@fontsource/source-code-pro": "^5.2.7",
    "@fontsource/space-grotesk": "^5.2.10",
    "@loadable/component": "^5.16.4",
    "@react-hook/mouse-position": "^4.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "dotenv": "^17.2.3",
    "gatsby": "^5.13.7",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-plugin-sitemap": "^6.13.1",
    "gatsby-plugin-styled-components": "^6.13.1",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-sharp": "^5.13.1",
    "gsap": "^3.12.5",
    "normalize.css": "^8.0.1",
    "particles-bg": "^2.5.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "styled-components": "^6.1.8",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/loadable__component": "^5.13.10",
    "@types/react": "^18.2.48",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-plugin-netlify": "^5.1.1",
    "lint-staged": "^15.2.0",
    "plop": "^4.0.1",
    "postcss-styled-syntax": "^0.7.1",
    "prettier": "^3.2.4",
    "stylelint": "^16.2.0",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "NODE_OPTIONS='--openssl-legacy-provider' npx tsc && NODE_OPTIONS='--openssl-legacy-provider' npx lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": "eslint --fix",
    "src/**/*.ts": "stylelint"
  }
}
